{
  "manifest_version": 2,

  "name": "Tabify",
  "description": "This extension just navigates to whatever page you want when you open a new tab",
  "version": "1.0",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/icon16.png",
    "default_popup": "popup.html",
    "default_title": "Tabify"
  },
  "icons":{
    "16" : "images/icon16.png",
    "48" : "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": ["tabs"]
}
